{
    "manifest_version": 3,
    "name": "FormCompleter",
    "description": "test",
    "version": "0.1",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["*://*/*"],
        "js": ["contentScript.js"]
    }],
    "permissions": ["activeTab", "tabs", "scripting", "contextMenus"]
}